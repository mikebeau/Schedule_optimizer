<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scheduleoptimizer"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Course1Preferences"
            android:label="@string/title_activity_course1_preferences"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ScheduleDisplay"
            android:label="@string/title_activity_schedule_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PreferencesMainMenu"
            android:label="@string/title_activity_preferences_main_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RankCourses"
            android:label="@string/title_activity_rank_courses"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SelectedClasses"
            android:label="@string/title_activity_selected_classes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SavedSchedules"
            android:label="@string/title_activity_saved_schedules"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Information" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>